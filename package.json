{
  "name": "naf-service",
  "version": "1.0.0",
  "description": "naf common service",
  "private": true,
  "egg": {
    "framework": "naf-framework-mongoose"
  },
  "dependencies": {
    "egg-naf-amqp": "0.0.13",
    "egg-redis": "^2.0.0",
    "jsonwebtoken": "^8.3.0",
